{
  "name": "Solarized Osaka",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": {
      "foreground": "#859900"
    },
    "variable.constant": {
      "foreground": "#d33682"
    },
    "variable.defaultLibrary": {
      "foreground": "#cb4b16"
    },
    "variable.declaration": {
      "foreground": "#839496"
    },
    "variable": {
      "foreground": "#839496"
    },
    "property.declaration": {
      "foreground": "#268bd2"
    },
    "property.defaultLibrary": {
      "foreground": "#268bd2"
    },
    "namespace": "#dc322f",
    "property": "#268bd2",
    "interface": "#268bd2",
    "type": "#268bd2",
    "class": "#b58900"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#586E75"
      }
    },
    {
      "name": "Number, Boolean, Undefined, Null",
      "scope": [
        "variable.other.constant",
        "punctuation.definition.constant",
        "constant.language",
        "constant.numeric",
        "support.constant"
      ],
      "settings": {
        "foreground": "#2AA198"
      }
    },
    {
      "name": "Symbols",
      "scope": [
        "constant.other.symbol",
        "constant.other.key",
        "meta.attribute-selector"
      ],
      "settings": {
        "foreground": "#CB4B16"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#2AA198"
      }
    },
    {
      "name": "Constant variables",
      "scope": "entity.name.constant",
      "settings": {
        "foreground": "#2AA198"
      }
    },
    {
      "name": "Escape Characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#A13C11"
      }
    },
    {
      "name": "Storage Type - int, float, bool,...",
      "scope": "storage.type",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "Storage - modifier, var, const, let",
      "scope": ["meta.var.expr storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "Keyword others (env,...)",
      "scope": "keyword.other",
      "settings": {
        "foreground": "#2AA198"
      }
    },
    {
      "name": "Spread",
      "scope": ["keyword.operator.spread", "keyword.operator.rest"],
      "settings": {
        "foreground": "#CB4B16"
      }
    },
    {
      "name": "if, try, end and while; operators",
      "scope": [
        "keyword.control",
        "keyword.control.conditional",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "Import, Export, From, Default",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.default",
        "meta.import keyword.other"
      ],
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword.control",
        "keyword.operator",
        "keyword.other.important",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "Keyword Control",
      "scope": [
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.instanceof"
      ],
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "Keyword SQL",
      "scope": "keyword.other.DML",
      "settings": {
        "foreground": "#2AA198"
      }
    },
    {
      "name": "Tag attributes",
      "scope": [
        "entity.other.attribute-name.html",
        "entity.other.attribute-name",
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#268BD2"
      }
    },
    {
      "name": "Tag",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "Tag Component",
      "scope": ["entity.name.tag support.class.component"],
      "settings": {
        "foreground": "#B58900"
      }
    },
    {
      "name": "Meta Tag",
      "scope": ["meta.tag"],
      "settings": {
        "foreground": "#268BD2"
      }
    },
    {
      "name": "Class types",
      "scope": "entity.name",
      "settings": {
        "foreground": "#268BD2"
      }
    },
    {
      "name": "Other types",
      "scope": ["meta.type", "support.type", "support.class"],
      "settings": {
        "foreground": "#B58900"
      }
    },
    // {
    //   "scope": ["meta.type.declaration", "meta.class", "storage.type.type"],
    //   "settings": {
    //     "foreground": "#859900"
    //   }
    // },
    {
